{
    "name": "reconengine/recon-laravel",
    "description": "Machine Learning brought to Laravel",
    "keywords": [
        "recon",
        "laravel",
        "machine learning",
        "neural network",
        "anomaly detection"
    ],
    "homepage": "https://github.com/reconengine/recon-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Recon Engine",
            "email": "contact@reconengine.ai",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Recon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Recon\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ReconServiceProvider"
            ],
            "aliases": {
                "Recon": "ReconFacade"
            }
        }
    }
}
