{
    "name": "exzachlyvv/laravel-ml",
    "description": "Machine Learning brought to Laravel",
    "keywords": [
        "laravel-ml",
        "machine learning",
        "neural network",
        "anomaly detection"
    ],
    "homepage": "https://github.com/exzachlyvv/laravel-ml",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Zachary Vander Velden",
            "email": "zachvv11@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/support": "^7.0.*|^8.0.*"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelMl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelMl\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelMl\\LaravelMlServiceProvider"
            ],
            "aliases": {
                "LaravelMl": "LaravelMl\\LaravelMlFacade"
            }
        }
    }
}
